<!DOCTYPE html>

@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    GC.Collect();
    String SessionID = HttpContextAccessor.HttpContext.Request.Cookies["SessionID"];

    if (String.IsNullOrEmpty(SessionID) || String.IsNullOrEmpty(Classes.UserFunctions.getUserID(SessionID)))
    {
        String sid = Classes.UserFunctions.generateSessionID();
        Classes.DBFunctions.sendQuery("INSERT INTO Guests (UserID, SessionID) VALUES('" + Classes.UserFunctions.generateSessionID() + "', '" + sid + "');");
        HttpContextAccessor.HttpContext.Response.Cookies.Append("SessionID", sid);
    }

    if (HttpContextAccessor.HttpContext.Request.Path.ToString().ToLower().Contains("/admin")
        && !Classes.UserFunctions.isAdmin(Classes.UserFunctions.getUserID(SessionID)))
    {
        HttpContextAccessor.HttpContext.Response.Redirect("/");
    }

    int day = DateTime.Now.DayOfYear;

}

